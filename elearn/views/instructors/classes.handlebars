<h2>{{instructor.firstName}}'s Classes</h2>
<table cellspacing="0" cellpadding='0')>
    <thead>
        <tr>
            <th>Class ID</th>
            <th>Class Name</th>
            <th>__</th>
        </tr>
    </thead>
    <tbody>
        {{#each instructor.class}}
            <tr>
                <td>{{class_id}}</td>
                <td>{{class_title}}</td>
                <td><a href="/classes/{{class_id}}/lessons">View Lessons</a></td>
            </tr>
        {{/each}}
    </tbody>
</table>